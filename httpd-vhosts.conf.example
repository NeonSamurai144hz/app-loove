# httpd-vhosts.conf.example
# Copy this file to your Apache configuration directory and rename it to httpd-vhosts.conf

<VirtualHost _default_:80>
  ServerName localhost
  ServerAlias localhost
  DocumentRoot "${INSTALL_DIR}/www"
  <Directory "${INSTALL_DIR}/www/">
    Options +Indexes +Includes +FollowSymLinks +MultiViews
    AllowOverride All
    Require local
  </Directory>
</VirtualHost>

<VirtualHost *:80>
    ServerName sweep.local
    DocumentRoot "C:/Coding/Dating-app/app-loove/frontend" // path to your frontend directory
    SetEnv APPLICATION_ENV "development"

    <Directory "C:/Coding/Dating-app/app-loove/frontend"> // path to your frontend directory
        Options FollowSymLinks MultiViews
        AllowOverride All
        Require all granted
    </Directory>

    ErrorLog "logs/sweep-error.log"
    CustomLog "logs/sweep-access.log" common
</VirtualHost>